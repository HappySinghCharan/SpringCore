<?xml version="1.0" encoding="utf-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns="http://www.springframework.org/schema/beans"

       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:component-scan base-package="org.autowireannotation"></context:component-scan>
    <context:annotation-config />
    <!-- this is our beans -->

    <bean class="org.autowireannotation.Address" name="address0">
        <property name="street" value="new street0" />
        <property name="city" value="Jaipur0" />
    </bean>

    <bean class="org.autowireannotation.Address" name="address1">
        <property name="street" value="new street1" />
        <property name="city" value="Jaipur1" />
    </bean>

    <bean class="org.autowireannotation.Emp" name="emp1"/>


</beans>

